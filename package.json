{
  "name": "productmicroservice",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/RogerioRomualdo/ProductMicroService.git",
  "author": "RogerioRomualdo <rogerioromualdo12@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsnd --respawn --transpile-only --ignore-watch node_modules src/index.ts",
    "start": "npm run build && cross-env NODE_ENV=release node dist/src/index.js",
    "build": "tsc",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "typeorm:run": "npm run typeorm migration:run",
    "test": "jest"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.5.3",
    "@grpc/proto-loader": "^0.6.9",
    "cross-env": "^7.0.3",
    "jest": "^27.4.7",
    "mysql2": "^2.3.3",
    "typeorm": "^0.2.41",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "ts-node-dev": "^1.1.8"
  }
}
